{
  "name": "api-sparx",
  "version": "1.0.0",
  "main": "index.js",
  "_moduleAliases": {
    "@": "src"
  },
  "prisma": {
    "seed": "ts-node src/migration/seed.ts"
  },
  "scripts": {
    "build": "npx tsc && npx tsc-alias",
    "start": "node ./dist/src/server.js",
    "dev": "nodemon src/server.ts",
    "prisma:generate": "npx prisma generate --schema=./src/migration/schema.prisma",
    "db:push": "npx prisma db push --schema=./src/migration/schema.prisma",
    "seed": "npx prisma db seed",
    "test": "cross-env NODE_ENV=test npx prisma generate --schema=./src/migration/schema.prisma &&  cross-env NODE_ENV=tes jest --detectOpenHandles --runInBand"
  },
  "keywords": [],
